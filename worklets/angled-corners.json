{
  "workletName": "Angled Corners",
  "packageName": "angled-corners",
  "npmURL": "https://www.npmjs.com/package/angled-corners",
  "workletUrl": "https://raw.githubusercontent.com/una/angled-corners/master/angled-corners.js",
  "cdnUrl": "https://unpkg.com/angled-corners",
  "demoUrl": "https://glitch.com/~angled-corners",
  "customProps": {
    "--corner-radius" : {
      "type": "string",
      "default": "0 20 0 0"
    },
    "--paint-color" : {
      "type": "color",
      "default": "#6200ee"
    },
    "--stroke-weight": {
      "type": "number",
      "default": "4",
      "range": [0, 15]
    },
    "--fill-type" : {
      "type": "string",
      "default": "filled",
      "options": ["filled", "outline"]
    }
  },
  "usage": "border-image: paint(angled-corners, outlined)",
  "html": "<button contenteditable style='outline: none; border: none; padding: 1rem; font-size: 1.25rem;'>Button</button>",
  "author": {
    "name": "@Una",
    "url": "https://twitter.com/una",
    "image": "https://webdev.imgix.net/images/authors/una@2x.jpg"
  }
}