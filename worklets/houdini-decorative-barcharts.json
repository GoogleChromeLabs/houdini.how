{
  "workletName": "Houdini Decorative Bar Charts",
  "packageName": "houdini-decorative-barcharts",
  "npmUrl": "https://www.npmjs.com/package/houdini-decorative-barcharts",
  "cdnUrl": "https://unpkg.com/houdini-decorative-barcharts",
  "demoUrl": "https://codepen.io/5t3ph/pen/jOwKGPZ",
  "tags": ["paint"],
  "customProps": {
    "--barchart-number": {
      "type": "number",
      "default": "5",
      "range": [1, 100]
    },
    "--barchart-seed": {
      "type": "number",
      "default": 24,
      "range": [0, 1000]
    },
    "--barchart-start-color": {
      "type": "color",
      "default": "#8c5af2"
    },
    "--barchart-end-color": {
      "type": "color",
      "default": "#400da5"
    }
  },
  "html": "<div style='width: 85%; height: 65%;'></div>",
  "usage": {
    "background-image": "paint(houdiniBarCharts)"
  },
  "author": {
    "name": "Stephanie Eckles",
    "url": "https://twitter.com/5t3ph",
    "image": "/assets/images/steph.jpg"
  }
}
