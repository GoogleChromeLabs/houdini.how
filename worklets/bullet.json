{
  "workletName": "bullet",
  "packageName": "css-houdini-bullet",
  "npmUrl": "https://www.npmjs.com/package/css-houdini-bullet",
  "cdnUrl": "https://unpkg.com/css-houdini-bullet@latest/public/worklet.js",
  "demoUrl": "https://codepen.io/edalgrin/pen/bGwOeqY",
  "tags": ["paint"],
  "customProps": {
    "--bullet-bg": {
      "type": "string",
      "default": "#3112c9, #12c99b"
    },
    "--bullet-items": {
      "type": "number",
      "default": "10",
      "range": [1, 50]
    },
    "--bullet-type": {
      "type": "number",
      "default": "0",
      "range": [0, 10]
    },
    "--bullet-curve": {
      "type": "number",
      "default": "0.5",
      "range": [0.1, 1],
      "step": "0.1"
    },
    "--bullet-size": {
      "type": "string",
      "default": "10, 30"
    },
    "--bullet-border": {
      "type": "number",
      "default": "0",
      "range": [0, 20]
    }
  },
  "usage": {
    "background": "paint(bullet)"
  },
  "author": {
    "name": "Eduardo Allegrini",
    "url": "https://twitter.com/edalgrin",
    "image": "/assets/images/eduardoallegrini.jpg"
  }
}
